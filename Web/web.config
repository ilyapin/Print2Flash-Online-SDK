<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <remove name="PHP5FastCGI" />
            <add name="PHP7-Fast-CGI" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="E:\php\php-cgi.exe" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
